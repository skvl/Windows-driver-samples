{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "shell",
			"label": "msbuild: clean build",
			"command": "msbuild",
			"args": [
				"/bl",
				"${workspaceFolder}/delete.vcxproj"
			],
			"problemMatcher": [
				"$msCompile"
			],
			"group": {
				"kind": "build",
				"isDefault": true
			}
		}
	]
}